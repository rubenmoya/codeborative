.u-container
  =link_to "New project", new_project_path
  %section.ProjectList
    - @projects.each do |project|
      %article.Project
        %section.Project-info.u-clear
          %h1=link_to project.name, project
          %p
            =icon('github-alt')
            =link_to clean_url(project.url), project.url
          %p=link_to "Edit", edit_project_path(project)
